{
	"format_version": "1.16.100",
	"minecraft:block": {
		"description": {
			"identifier": "extrabiomes:stripped_mystic_wood",
			"properties": {
				"extrabiomes:direction": {}
			}
		},
		"components": {
			"minecraft:creative_category": {
                "group": "itemGroup.name.wood",
                "category": "nature"
            },
			"minecraft:destroy_time": 0.5,
			"minecraft:unit_cube": {},
			"minecraft:on_player_placing": {
				"event": "extrabiomes:direction",
				"target": "self"
			},
			"minecraft:material_instances": {
				"*": {
					"texture": "mystic_stripped_log",
					"render_method": "opaque"
				}
			},
			"minecraft:flammable": {
				"burn_odds": 50,
				"flame_odds": 50
			},
			"minecraft:map_color": "#ff78f8",
			"tag:wood": {}
		},
		"permutations": [
			{
				"condition": "query.block_property('extrabiomes:direction') == 1",
				"components": {
					"minecraft:rotation": [
						90,
						0,
						0
					]
				}
			},
			{
				"condition": "query.block_property('extrabiomes:direction') == 2",
				"components": {
					"minecraft:rotation": [
						0,
						90,
						90
					]
				}
			}
		],
		"events": {
			"extrabiomes:direction": {
				"set_block_property":{
                    "extrabiomes:direction": "math.floor( query.block_face / 2.0 )"
                }
			}
		}
	}
}