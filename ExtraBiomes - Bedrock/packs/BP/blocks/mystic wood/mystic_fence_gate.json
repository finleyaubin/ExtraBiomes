{
  "format_version": "1.20.100",
  "minecraft:block": {
    "description": {
      "identifier": "extrabiomes:mystic_fence_gate",
      "properties": {
        "extrabiomes:facing": [
          0,
          1,
          2,
          3
        ],
        "extrabiomes:open_bit": [
          0,
          1
        ],
        "extrabiomes:mirror_bit": [
          0,
          1
        ]
      },
