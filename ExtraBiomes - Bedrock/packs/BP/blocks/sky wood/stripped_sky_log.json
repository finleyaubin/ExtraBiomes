{
  "format_version": "1.20.100",
  "minecraft:block": {
    "description": {
      "identifier": "extrabiomes:stripped_sky_log",
      "properties": {
        "extrabiomes:direction": [
          0,
          1,
          2
        ]
      },
      "menu_category": {
        "category": "nature",
        "group": "itemGroup.name.log"
      }
    },
    "components": {
      "minecraft:on_player_placing": {
        "event": "extrabiomes:direction",
        "target": "self"
      },
      "minecraft:material_instances": {
        "*": {
          "texture": "sky_stripped_log",
          "render_method": "opaque"
        },
        "up": {
          "texture": "sky_stripped_log_top",
          "render_method": "opaque"
        },
        "down": {
          "texture": "sky_stripped_log_top",
          "render_method": "opaque"
        }
      },
      "minecraft:flammable": {
        "burn_odds": 50,
        "flame_odds": 50
      },
      "minecraft:map_color": "#ff78f8",
      "tag:wood": {},
      "minecraft:destructible_by_mining": {
        "seconds_to_destroy": 0.5
      }
    },
    "permutations": [
      {
        "condition": "query.block_property('extrabiomes:direction') == 0",
        "components": {
          "minecraft:rotation": [
            0,
            0,
            0
          ]
        }
      },
      {
        "condition": "query.block_property('extrabiomes:direction') == 1",
        "components": {
          "minecraft:rotation": [
            90,
            0,
            0
          ]
        }
      },
      {
        "condition": "query.block_property('extrabiomes:direction') == 2",
        "components": {
          "minecraft:rotation": [
            0,
            0,
            90
          ]
        }
      }
    ],
    "events": {
      "extrabiomes:direction": {
        "set_block_property": {
          "extrabiomes:direction": "math.floor( query.cardinal_block_face_placed_on  / 2.0 )"
        }
      }
    }
  }
}
