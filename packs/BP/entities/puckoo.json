{
  "format_version": "1.20.0",
  "minecraft:entity": {
    "description": {
      "identifier": "extrabiomes:puckoo",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false,
      "runtime_identifier": "minecraft:sheep",
      "properties": {
        "extrabiomes:gender": {
          "type": "enum",
          "values": [ "male", "female" ],
          "default": "male",
          "client_sync": true
        }
      }

    },
    "component_groups": {
      "extrabiomes:puckoo_baby": {
        "minecraft:is_baby": {
        },
        "minecraft:scale": {
          "value": 0.5
        },
        "minecraft:ageable": {
          "duration": 1200,
          "feed_items": [
            "extrabiomes:mossy_pebble"
          ],
          "grow_up": {
            "event": "minecraft:ageable_grow_up",
            "target": "self"
          }
        },
        "minecraft:behavior.follow_parent": {
          "priority": 5,
          "speed_multiplier": 1.1
        }
      },

      "extrabiomes:puckoo_adult": {
        "minecraft:experience_reward": {
          "on_bred": "Math.Random(1,7)",
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/chicken.json"
        },
        "minecraft:breedable": {
          "require_tame": false,
          "breeds_with": {
            "mate_type": "extrabiomes:puckoo",
            "baby_type": "extrabiomes:puckoo",
            "breed_event": {
              "event": "minecraft:entity_born",
              "target": "baby"
            }
          },
          "breed_items": [
            "extrabiomes:mossy_pebble"
          ]
        },
        "minecraft:behavior.breed": {
          "priority": 3,
          "speed_multiplier": 1.0
        },
        "minecraft:rideable": {
          "seat_count": 1,
          "family_types": [
            "player"
          ],
          "seats": {
            "position": [0, 0.5, -0.1]
          }
        }
      },
      "extrabiomes:puckoo_calm": {
        "minecraft:on_target_acquired": {
          "event": "extrabiomes:become_scared",
          "target": "self"
        },
        "minecraft:movement": {
          "value": 0.25
        }
      },
      "extrabiomes:drop_pebble": {
        "minecraft:spawn_entity": {
          "entities": {
            "min_wait_time": 7,
            "max_wait_time": 7,
            "spawn_item": "extrabiomes:pebble",
            "spawn_item_event": {
              "event": "extrabiomes:on_drop",
              "target": "self"
            }   
          }
        }
      },

      "extrabiomes:puckoo_scared": {
        "minecraft:angry": {
          "duration": 4,
          "calm_event": {
            "event": "extrabiomes:puckoo_become_calm",
            "target": "self"
          }
        },
        "minecraft:movement": {
          "value": 0
        }
      },
      "extrabiomes:skin_0": {
        "minecraft:skin_id": {
          "value": 0
        }
      },
      "extrabiomes:skin_1": {
        "minecraft:skin_id": {
          "value": 1
        }
      },
      "extrabiomes:skin_2": {
        "minecraft:skin_id": {
          "value": 2
        }
      },
      "extrabiomes:skin_3": {
        "minecraft:skin_id": {
          "value": 3
        }
      },
      "extrabiomes:koi_0": {
        "minecraft:variant": {
          "value": 0
        }
      },
      "extrabiomes:koi_1": {
        "minecraft:variant": {
          "value": 1
        }
      },
      "extrabiomes:koi_2": {
        "minecraft:variant": {
          "value": 2
        }
      },
      "extrabiomes:koi_3": {
        "minecraft:variant": {
          "value": 3
        }
      }
    },

    "components": {
      "minecraft:is_hidden_when_invisible": {
      },
      "minecraft:type_family": {
        "family": [ "puckoo", "fish", "chicken", "mob" ]
      },
      "minecraft:breathable": {
        "total_supply": 60,
        "suffocate_time": 0
      },
      "minecraft:collision_box": {
        "width": 0.8,
        "height": 1
      },
      "minecraft:nameable": {
      },
      "minecraft:health": {
        "value": 6,
        "max": 6
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": { "test": "in_lava", "subject": "self", "operator": "==", "value": true },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 3
      },
      "minecraft:movement": {
        "value": 0.25
      },
      "minecraft:damage_sensor": {
        "triggers": {
          "cause": "fall",
          "deals_damage": false
        }
      },
      "minecraft:leashable": {
        "soft_distance": 4.0,
        "hard_distance": 6.0,
        "max_distance": 10.0
      },
      "minecraft:balloonable": {
        "mass": 0.5
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": true,
        "avoid_damage_blocks": true
      },
      "minecraft:movement.basic": {
      },
      "minecraft:jump.static": {
      },
      "minecraft:can_climb": {
      },
      "minecraft:despawn": {
        "despawn_from_distance": { }
      },
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.panic": {
        "priority": 1,
        "speed_multiplier": 1.5
      },

      "minecraft:behavior.mount_pathing": {
        "priority": 2,
        "speed_multiplier": 1.5,
        "target_dist": 0.0,
        "track_target": true
      },
      "minecraft:behavior.tempt": {
        "priority": 4,
        "speed_multiplier": 1.0,
        "items": [
          "extrabiomes:mossy_pebble"
        ]
      },
      "minecraft:lookat": {
        "search_radius": 3.0,
        "set_target": true,
        "look_cooldown": 50.0,
        "filters": {
          "any_of": [
            {
              "subject": "other",
              "test": "is_family",
              "value": "player"
            },
            {
              "subject": "other",
              "test": "is_family",
              "value": "puckoo"
            }
          ]
        }
      },
      "minecraft:behavior.random_stroll": {
        "priority": 6,
        "speed_multiplier": 1.0
      },
      "minecraft:behavior.look_at_player": {
        "priority": 7,
        "look_distance": 6.0,
        "probability": 8.0
      },
      "minecraft:behavior.random_look_around": {
        "priority": 8
      },
      "minecraft:physics": {
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:conditional_bandwidth_optimization": {
      },
      "minecraft:game_event_movement_tracking": {
        "emit_flap": true
      },
      "minecraft:behavior.eat_block": {
        "priority": 6,
        "success_chance": "query.is_baby ? 0.02 : 0.01",
        "eat_and_replace_block_pairs": [
          { "eat_block": "extrabiomes:large_mossy_pebble", "replace_block": "extrabiomes:medium_mossy_pebble" },
          { "eat_block": "extrabiomes:medium_mossy_pebble", "replace_block": "extrabiomes:small_mossy_pebble" },
          { "eat_block": "extrabiomes:small_mossy_pebble", "replace_block": "air" }
        ],
        "time_until_eat": 0,
        "on_eat": {
          "event": "extrabiomes:drop",
          "target": "self"
        }

      }
    },

    "events": {
      "from_egg": {
        "add": { "component_groups": [ "extrabiomes:puckoo_baby" ] }
      },
      "extrabiomes:drop": {
        "add": { "component_groups": [ "extrabiomes:drop_pebble" ] }
      },
      "extrabiomes:on_drop": {
        "remove": { "component_groups": [ "extrabiomes:drop_pebble" ] }
      },

      "minecraft:entity_spawned": {
        "sequence": [
          {
            "randomize": [
              {
                "weight": 95,
                "trigger": "minecraft:spawn_adult"
              },
              {
                "weight": 5,
                "add": {
                  "component_groups": [
                    "extrabiomes:puckoo_baby"
                  ]
                }
              }
            ]
          },
          {
            "randomize": [
              {
                "weight": 50,
                "add": {
                  "component_groups": [
                    "extrabiomes:skin_0"
                  ]
                }
              },
              {
                "weight": 20,
                "add": {
                  "component_groups": [
                    "extrabiomes:skin_1"
                  ]
                }
              },
              {
                "weight": 20,
                "add": {
                  "component_groups": [
                    "extrabiomes:skin_2"
                  ]
                }
              },
              {
                "weight": 10,
                "add": {
                  "component_groups": [
                    "extrabiomes:skin_3"
                  ]
                }
              }
            ]
          },
          {
            "randomize": [
              {
                "weight": 50,
                "add": {
                  "component_groups": [
                    "extrabiomes:koi_0"
                  ]
                }
              },
              {
                "weight": 20,
                "add": {
                  "component_groups": [
                    "extrabiomes:koi_1"
                  ]
                }
              },
              {
                "weight": 20,
                "add": {
                  "component_groups": [
                    "extrabiomes:koi_2"
                  ]
                }
              },
              {
                "weight": 10,
                "add": {
                  "component_groups": [
                    "extrabiomes:koi_3"
                  ]
                }
              }
            ]
          },
          {
            "randomize": [
              {
                "weight": 1,
                "set_property": {
                  "extrabiomes:gender": "male"
                }
              },
              {
                "weight": 1,
                "set_property": {
                  "extrabiomes:gender": "female"
                }
              }
            ]
          },
          {
            "add": {
              "component_groups": [
                "extrabiomes:puckoo_calm"
              ]
            }
          }
        ]
      },

      "minecraft:entity_born": {
        "remove": {
        },
        "add": {
          "component_groups": [
            "extrabiomes:puckoo_baby"
          ]
        }
      },

      "minecraft:ageable_grow_up": {
        "remove": {
          "component_groups": [
            "extrabiomes:puckoo_baby"
          ]
        },
        "add": {
          "component_groups": [
            "extrabiomes:puckoo_adult"
          ]
        }
      },

      "minecraft:spawn_adult": {
        "add": {
          "component_groups": [
            "extrabiomes:puckoo_adult"
          ]
        }
      },
      "extrabiomes:become_scared": {
        "remove": {
          "component_groups": [
            "extrabiomes:puckoo_calm"
          ]
        },
        "add": {
          "component_groups": [
            "extrabiomes:puckoo_scared"
          ]
        }
      },
      "extrabiomes:puckoo_become_calm": {
        "remove": {
          "component_groups": [
            "extrabiomes:puckoo_scared"
          ]
        },
        "add": {
          "component_groups": [
            "extrabiomes:puckoo_calm"
          ]
        }
      }
    }
  }
}
